{
  "name": "kaibanjs",
  "version": "0.8.2",
  "description": "AI Multi-Agent library for Javascript Developers with Groq integration.",
  "main": "./dist/bundle.cjs",
  "types": "./dist/bundle.d.ts",
  "module": "./dist/bundle.mjs",
  "unpkg": "./dist/bundle.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json",
    "xscripts/cli.mjs"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/bundle.d.ts",
        "default": "./dist/bundle.mjs"
      },
      "require": {
        "types": "./dist/bundle.d.ts",
        "default": "./dist/bundle.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:types && npx rollup -c --bundleConfigAsCjs",
    "build:test": "cross-env NODE_ENV=development TEST_ENV=mocked-llm-apis npx rollup -c --bundleConfigAsCjs",
    "dev": "cross-env NODE_ENV=development npx rollup -c -w --bundleConfigAsCjs",
    "test": "npm run build:test && npm run test:integration",
    "test:watch": "cross-env TEST_ENV=mocked-llm-apis jest --testPathPattern='tests/e2e' --watch",
    "test:debug": "cross-env TEST_ENV=mocked-llm-apis node --inspect-brk node_modules/.bin/jest --runInBand --verbose --testPathPattern='tests/e2e'",
    "test:prod": "npm run build && jest --testPathPattern='tests/e2e'",
    "test:integration": "cross-env TEST_ENV=mocked-llm-apis jest --testPathPattern='tests/e2e'",
    "test:e2e": "cross-env TEST_ENV=real-llm-apis jest --testPathPattern='tests/e2e'",
    "test:unit": "jest --testPathPattern='tests/unit' --watch",
    "test:unit:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --verbose --testPathPattern='tests/unit'",
    "test:types": "npx tsc",
    "play:react": "cd playground/react && npm run dev",
    "play:sb": "cd playground/react && npm run storybook",
    "play:node": "cd playground/nodejs && node index.js",
    "code-snapshot": "node xscripts/codeSnapshot.js",
    "cli": "node ./xscripts/cli.mjs",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "bin": {
    "kaiban": "./xscripts/cli.mjs"
  },
  "packageManager": "npm@9.8.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Walchshofer/KaibanJS.git"
  },
  "author": "AI Champions",
  "license": "MIT",
  "homepage": "https://github.com/Walchshofer/KaibanJS#readme",
  "bugs": {
    "url": "https://github.com/Walchshofer/KaibanJS/issues"
  },
  "keywords": [
    "ai agents",
    "multi-agent systems",
    "agentic",
    "javascript",
    "framework",
    "node.js",
    "kaibanjs",
    "langchain",
    "workflow automation",
    "task orchestration",
    "autonomous agents",
    "collaborative ai",
    "ai-powered applications",
    "human-ai interaction",
    "ai workflow",
    "large language models",
    "llm",
    "prompt engineering",
    "react",
    "nodejs",
    "nextjs",
    "groq"
  ],
  "dependencies": {
    "ansis": "^3.3.2",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "figlet": "^1.7.0",
    "loglevel": "^1.9.1",
    "ora": "^8.1.0",
    "p-queue": "^8.0.1",
    "readline": "^1.3.0",
    "uuid": "10.0.0",
    "zustand": "4.5.2"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.29.0",
    "@langchain/anthropic": "^0.3.6",
    "@langchain/community": "^0.3.10",
    "@langchain/core": "^0.3.15",
    "@langchain/google-genai": "^0.1.2",
    "@langchain/groq": "^0.1.2",
    "@langchain/mistralai": "^0.1.1",
    "@langchain/openai": "^0.3.11",
    "groq-sdk": "^0.7.0",
    "langchain": "^0.3.5",
    "openai": "^4.67.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.29.0",
    "@babel/core": "^7.24.9",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.24.8",
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@langchain/anthropic": "^0.3.6",
    "@langchain/community": "^0.3.10",
    "@langchain/core": "0.3.15",
    "@langchain/google-genai": "^0.1.2",
    "@langchain/groq": "^0.1.2",
    "@langchain/mistralai": "^0.1.1",
    "@langchain/openai": "^0.3.11",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "26.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/init-package-json": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "eslint-plugin-office-addins": "^3.0.2",
    "groq-sdk": "^0.7.0",
    "jest": "29.7.0",
    "langchain": "^0.3.5",
    "openai": "^4.67.3",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "tslib": "^2.7.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
